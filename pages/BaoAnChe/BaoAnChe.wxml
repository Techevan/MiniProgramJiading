<!--Jiading.wxml-->
<view>
  <view class="title">保安车</view>
  
  <picker-view indicator-style="height: 50px;" style="width: 100%; height: 220px;" value="{{value}}" bindchange="bindChange1">
    <picker-view-column>
      <view class="contactInfo" wx:for="{{contactInfo}}" style="line-height: 50px">{{item}}</view>
    </picker-view-column>
  </picker-view>
  <button type="primary" bindtap="setCall"><view class="botton">给保安车师傅打电话</view></button>
</view>